@server {
  @plane("ripple") {
    class: "swim.ripple.RipplePlane"
  }
  @store {
    path: "/tmp/swim-ripple"
  }
  @http(port: 9001) {
    plane: "ripple"
    documentRoot: "./ui/"
    @websocket {
      serverCompressionLevel: 0# -1 = default; 0 = off; 1-9 = deflate level
      clientCompressionLevel: 0# -1 = default; 0 = off; 1-9 = deflate level
    }
  }
}
